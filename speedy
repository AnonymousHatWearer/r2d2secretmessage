game.Players.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        -- Wait for the humanoid (the part of the character that controls movement)
        local humanoid = character:WaitForChild("Humanoid")
        
        -- Set the walk speed to an abnormally high value (e.g., 200)
        humanoid.WalkSpeed = 40  -- Default walk speed is 16, 30 is much faster
    end)
end)
